# See https://help.github.com/ignore-files/ for more about ignoring files.

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore database config files
/config/database.yml

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore local environment config
/config/credentials/*
/config/puma.rb
/config/secrets.yml

# Ignore dotenv
/.env*

# Ignore yarn and node_modules
/node_modules
/yarn.lock

# Ignore pids and sockets
/tmp/pids/
/tmp/sockets/

# Ignore cache files
/public/packs
/public/packs-test
/public/assets

# Ignore uploaded files
/storage

# Ignore .byebug_history
.byebug_history

# Ignore coverage files
/coverage/*

# Ignore generated docs
/doc/*

# Ignore generated files
/public/packs
/public/packs-test
/public/assets

# Ignore test files
/test/*
/spec/*

# Ignore guard files
/Guardfile

# Ignore local environment files
/.env.local
/.env.*.local

# Ignore local history of files
/.*.local

# Ignore local test files
/spec/examples.txt

# Ignore spring files
/spring/*.pid